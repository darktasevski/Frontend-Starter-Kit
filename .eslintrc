{
  "env": {
    "es6": true,
    "node": true,
    "jquery": true,
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "indent": [2, 4],
    "semi": [ 2, "always" ],
    "arrow-body-style": [2, "as-needed"],
    "no-param-reassign": [2, {"props": false}],
    "import": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "comma-dangle": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "react/ptefer-es6-class": 0,
    "radix": 0,
    "no-console": 0,
    "no-unused-vars": [1, {"argsIgnorePattern":
        "req|res|next|^err"
    }]
  },
  "plugins": ["html", "markdown"]
}

